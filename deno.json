{
  "name": "@siguici/buno",
  "version": "0.1.1",
  "exports": {
    ".": "./jsr/index.ts",
    "./assert": "./jsr/assert.ts",
    "./async_hooks": "./jsr/async_hooks.ts",
    "./buffer": "./jsr/buffer.ts",
    "./child_process": "./jsr/child_process.ts",
    "./console": "./jsr/console.ts",
    "./crypto": "./jsr/crypto.ts",
    "./dgram": "./jsr/dgram.ts",
    "./diagnostics_channel": "./jsr/diagnostics_channel.ts",
    "./dns": "./jsr/dns.ts",
    "./events": "./jsr/events.ts",
    "./fs": "./jsr/fs.ts",
    "./http": "./jsr/http.ts",
    "./http2": "./jsr/http2.ts",
    "./https": "./jsr/https.ts",
    "./module": "./jsr/module.ts",
    "./net": "./jsr/net.ts",
    "./os": "./jsr/os.ts",
    "./path": "./jsr/path.ts",
    "./perf_hooks": "./jsr/perf_hooks.ts",
    "./process": "./jsr/process.ts",
    "./punycode": "./jsr/punycode.ts",
    "./querystring": "./jsr/querystring.ts",
    "./readline": "./jsr/readline.ts",
    "./stream": "./jsr/stream.ts",
    "./string_decoder": "./jsr/string_decoder.ts",
    "./timers": "./jsr/timers.ts",
    "./tls": "./jsr/tls.ts",
    "./tty": "./jsr/tty.ts",
    "./url": "./jsr/url.ts",
    "./util": "./jsr/util.ts",
    "./vm": "./jsr/vm.ts",
    "./worker_threads": "./jsr/worker_threads.ts",
    "./zlib": "./jsr/zlib.ts"
  },
  "include": ["./deno.json", "jsr/*.ts", "README.md"],
  "exclude": [
    ".*",
    "jsr/*.test.ts",
    "js/*",
    "ts/*",
    "src/*",
    "scripts/*",
    "tests/*",
    "biome.json",
    "tsconfig.json",
    "deno.lock"
  ],
  "publish": {
    "include": [
      "jsr/**",
      "deno.json",
      "package.json",
      "tsconfig.json",
      "README.md",
      "LICENSE.md"
    ],
    "exclude": ["!jsr/*.ts"]
  },
  "compilerOptions": {
    "lib": [
      "esnext",
      "webworker",
      "deno.ns",
      "deno.window",
      "dom",
      "dom.iterable",
      "dom.asynciterable"
    ]
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true,
    "proseWrap": "preserve"
  },
  "tasks": {
    "check": "deno task check:fmt && deno task check:lint && deno task check:types",
    "check:fmt": "deno fmt --check",
    "check:lint": "deno lint",
    "check:types": "deno check jsr/*.ts",
    "fix": "deno lint --fix && deno fmt"
  }
}
