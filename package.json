    "cross-platform",
    "runtime",
    "adapter",
    "JavaScript",
    "TypeScript",
    "Node.js",
    "Bun",
    "Deno",
    "API",
    "compatibility"
  ],
  "module": "./js/index.js",
  "main": "./js/index.js",
  "types": "./ts/",
  "files": ["js/**/*", "ts/**/*"],
