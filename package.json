  "scripts": {
    "build": "bun run build.node && bun run build.deno",
    "build.node": "rimraf node/*.{js,ts,d.ts} && tsc --build && bun scripts/build.ts",
    "build.deno": "rimraf deno/*.{js,ts,d.ts,d.ts.map} && denoify --out deno && bun scripts/denoify.ts",
    "check": "biome ci . && knip",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "clear": "bun run clear.node && bun run clear.deno",
    "denoify": "denoify",
    "debug": "bun check && bun test",
    "fix": "bun run unsed && bun lint && bun format",
    "format": "biome format --write .",
    "knip": "knip",
    "lint": "biome check --write .",
    "unsed": "knip --fix"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
