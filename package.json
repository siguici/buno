  "name": "buno.js",
  "version": "0.1.1",
  "private": false,
  "sideEffects": true,
  "type": "module",
  "license": "MIT",
  "engines": {
    "bun": ">=1.1"
  },
  "packageManager": "bun@1.1.24",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/siguici/buno#readme",
  "repository": "https://github.com/siguici/buno",
  "bugs": "https://github.com/siguici/buno/issues",
  "author": {
    "name": "Sigui Kessé Emmanuel",
    "email": "siguici@proton.me",
    "url": "https://github.com/siguici"
  },
  "description": "⚙️  Cross-Platform JavaScript/TypeScript Runtime Adapter",
